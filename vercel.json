{
  "version": 2,
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
server: {
  allowedHosts: [process.env.REPLIT_DOMAINS || '.replit.dev'],
  fs: {
    strict: true,
    deny: ["**/.*"],
  },
},
  